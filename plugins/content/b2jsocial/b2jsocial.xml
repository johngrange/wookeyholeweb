								</field>
								<field name="icon_size" type="list" default="32" label="PLG_B2J_SOCIAL_ICON_SIZE"  description="PLG_B2J_SOCIAL_ICON_SIZE_DESC">
									<option value="16">PLG_B2J_SOCIAL_ICON_SIZE_16</option>
									<option value="32">PLG_B2J_SOCIAL_ICON_SIZE_32</option>
									<option value="64">PLG_B2J_SOCIAL_ICON_SIZE_64</option>
								</field>
								<field name="position" type="list" default="after" label="PLG_B2J_SOCIAL_POSITION"  description="PLG_B2J_SOCIAL_POSITION_DESC">
									<option value="after">PLG_B2J_SOCIAL_AFTER_CONTENT</option>
									<option value="before">PLG_B2J_SOCIAL_BEFORE_CONTENT</option>
									<option value="both">PLG_B2J_SOCIAL_AFTER_BEFORE_CONTENT</option>
								</field>
								<field name="align" type="list" default="left" label="PLG_B2J_SOCIAL_ALIGN"  description="PLG_B2J_SOCIAL_ALIGN_DESC">
									<option value="left">PLG_B2J_SOCIAL_LEFT</option>
									<option value="center">PLG_B2J_SOCIAL_CENTER</option>
									<option value="right">PLG_B2J_SOCIAL_RIGHT</option>
								</field>
                
                <field name="f_og" type="radio" default="1" label="PLG_B2J_SOCIAL_FACEBOOK_OG"  description="PLG_B2J_SOCIAL_FACEBOOK_OG_DESC">
                    <option value="1">PLG_B2J_SOCIAL_ON</option>
                    <option value="0">PLG_B2J_SOCIAL_OFF</option>
                </field>
                <field name="t_name" type="text" default="bang2joom" label="PLG_B2J_SOCIAL_TWITTER_NAME"  description="PLG_B2J_SOCIAL_TWITTER_NAME_DESC" />
                
                <field type="spacer" name="item_priority_" class="text" label="PLG_B2J_SOCIAL_ITEM_PRIORITY" description="PLG_B2J_SOCIAL_ITEM_PRIORITY_DESC" />
                
								<field name="f_pos" default="1" type="hidden" label=""></field>
                <field name="t_pos" default="2" type="hidden" label=""></field>
								<field name="g_pos" default="3" type="hidden" label=""></field>
								<field name="l_pos" default="4" type="hidden" label=""></field>
                <field name="p_pos" default="5" type="hidden" label=""></field>
								<field name="e_pos" default="6" type="hidden" label=""></field>
                <field name="f" type="radio" default="1" class="pos_field" label="PLG_B2J_SOCIAL_FACEBOOK">
                    <option value="1">PLG_B2J_SOCIAL_ON</option>
                    <option value="0">PLG_B2J_SOCIAL_OFF</option>
                </field>
									<field name="t" type="radio" default="1" class="pos_field" label="PLG_B2J_SOCIAL_TWITTER">
                    <option value="1">PLG_B2J_SOCIAL_ON</option>
                    <option value="0">PLG_B2J_SOCIAL_OFF</option>
                </field>
