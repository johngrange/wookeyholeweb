	<config>
        <fields name="params">
            <fieldset name="PLG_B2J_SOCIAL_BASIC">
                <field type="b2jupdate" addfieldpath="/plugins/content/b2jsocial/admin/elements/"></field>
                <field name="preset" type="list" default="1" label="PLG_B2J_SOCIAL_PRESET"  description="PLG_B2J_SOCIAL_PRESET_DESC">
									<option value="7">PLG_B2J_SOCIAL_PRESET_7</option>
									<option value="1">PLG_B2J_SOCIAL_PRESET_1</option>
									<option value="2">PLG_B2J_SOCIAL_PRESET_2</option>
									<option value="3">PLG_B2J_SOCIAL_PRESET_3</option>
									<option value="4">PLG_B2J_SOCIAL_PRESET_4</option>
									<option value="5">PLG_B2J_SOCIAL_PRESET_5</option>
									<option value="6">PLG_B2J_SOCIAL_PRESET_6</option>
									<option value="8">PLG_B2J_SOCIAL_PRESET_8</option>
									<option value="9">PLG_B2J_SOCIAL_PRESET_9</option>
									<option value="10">PLG_B2J_SOCIAL_PRESET_10</option>
								</field>
								<field name="share_counts" type="radio" default="0" label="PLG_B2J_SOCIAL_SHARE_COUNTS" description="PLG_B2J_SOCIAL_SHARE_COUNTS_DESC">
                    <option value="1">PLG_B2J_SOCIAL_ON</option>
                    <option value="0">PLG_B2J_SOCIAL_OFF</option>
                </field>
								<field name="count_color" type="text" default="#666666" class="color" label="PLG_B2J_SOCIAL_COUNTS_COLOR" description="PLG_B2J_SOCIAL_COUNTS_COLOR_DESC" />
								
								<field name="include_font" type="radio" default="1" label="PLG_B2J_SOCIAL_INCLUDE_FONT" description="PLG_B2J_SOCIAL_INCLUDE_FONT_DESC">
                    <option value="1">PLG_B2J_SOCIAL_ON</option>
                    <option value="0">PLG_B2J_SOCIAL_OFF</option>
                </field>
								<field name="background_color" type="text" default="#999999" class="color" label="PLG_B2J_SOCIAL_BACKGROUND_COLOR" description="PLG_B2J_SOCIAL_BACKGROUND_COLOR_DESC" />
								<field name="icon_color" type="text" default="#ffffff" class="color" label="PLG_B2J_SOCIAL_ICON_COLOR" description="PLG_B2J_SOCIAL_COUNTS_ICON_COLOR_DESC" />
								<field name="icon_font_size" type="text" default="24" label="PLG_B2J_SOCIAL_ICON_FONT_SIZE" description="PLG_B2J_SOCIAL_COUNTS_ICON_FONT_SIZE_DESC" />
								
								<field name="content" type="list" default="all" label="PLG_B2J_SOCIAL_CONTENT"  description="PLG_B2J_SOCIAL_CONTENT_DESC">
									<option value="k2">PLG_B2J_SOCIAL_K2</option>
									<option value="article">PLG_B2J_SOCIAL_JOOMLA</option>
									<option value="rs">PLG_B2J_SOCIAL_REDSHOP</option>
									<option value="all">PLG_B2J_SOCIAL_ALL</option>
								</field>
                <field name="k2_filter" type="radio" default="1" label="PLG_B2J_SOCIAL_K2_FILTER" description="PLG_B2J_SOCIAL_K2_FILTER_DESC">
                    <option value="1">PLG_B2J_SOCIAL_ALL</option>
                    <option value="0">PLG_B2J_SOCIAL_SELECT</option>
                </field>
                <field name="k2_categories" type="categoriesmultiple" default="" label="PLG_B2J_SOCIAL_SELECT_ONE_OR_MORE_CATEGORIES" description="PLG_B2J_SOCIAL_SELECT_ONE_OR_MORE_CATEGORIES_DESC" addfieldpath="/administrator/components/com_k2/elements/" />
                <field name="article_filter" type="radio" default="1" label="PLG_B2J_SOCIAL_ARTICLE_FILTER" description="PLG_B2J_SOCIAL_ARTICLE_FILTER_DESC">
                    <option value="1">PLG_B2J_SOCIAL_ALL</option>
                    <option value="0">PLG_B2J_SOCIAL_SELECT</option>
                </field>
                <field name="joomla_categories" multiple="true" type="category" default="" extension="com_content" label="PLG_B2J_SOCIAL_SELECT_ONE_OR_MORE_CATEGORIES_JOOMLA" description="PLG_B2J_SOCIAL_SELECT_ONE_OR_MORE_CATEGORIES_JOOMLA_DESC" addfieldpath="/libraries/joomla/form/fields/" />
                <field name="include_in_categories" type="radio" default="0" label="PLG_B2J_SOCIAL_INCLUDE_IN_CATEGORIES" description="PLG_B2J_SOCIAL_INCLUDE_IN_CATEGORIES_DESC">
                    <option value="1">PLG_B2J_SOCIAL_ON</option>
                    <option value="0">PLG_B2J_SOCIAL_OFF</option>
                </field>
                <field name="buttons" type="list" default="icons" label="PLG_B2J_SOCIAL_BUTTONS"  description="PLG_B2J_SOCIAL_BUTTONS_DESC">
									<option value="icons">PLG_B2J_SOCIAL_ICONS</option>
									<option value="text">PLG_B2J_SOCIAL_TEXT</option>
