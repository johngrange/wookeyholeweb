                    <option value="1">PLG_B2J_SOCIAL_ON</option>
                    <option value="0">PLG_B2J_SOCIAL_OFF</option>
                </field>
								<field name="l" type="radio" default="1" class="pos_field" label="PLG_B2J_SOCIAL_LINKEDIN">
                    <option value="1">PLG_B2J_SOCIAL_ON</option>
                    <option value="0">PLG_B2J_SOCIAL_OFF</option>
                </field>
                <field name="p" type="radio" default="1" class="pos_field" label="PLG_B2J_SOCIAL_PINTEREST">
                    <option value="1">PLG_B2J_SOCIAL_ON</option>
                    <option value="0">PLG_B2J_SOCIAL_OFF</option>
                </field>
								<field name="e" type="radio" default="1" class="pos_field" label="PLG_B2J_SOCIAL_EMAIL">
                    <option value="1">PLG_B2J_SOCIAL_ON</option>
                    <option value="0">PLG_B2J_SOCIAL_OFF</option>
                </field>
								<field type="b2jhidden" addfieldpath="/plugins/content/b2jsocial/admin/elements/"></field>
